	"repository": {
		"type": "git",
		"url": "https://github.com/nasa/vscode-pvs"
	},
	"publisher": "paolomasci",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"vscode-pvs",
		"pvs",
		"prototype verification system"
	],
	"engines": {
		"vscode": "^1.31.0"
	},
	"activationEvents": [
		"onLanguage:pvs"
	],
	"main": "./client/out/pvsLanguageClient",
	"contributes": {
		"notes": {
			"description": "These contributed settings can be read using vscode.workspace.getConfiguration('pvs')"
		},
		"configuration": [
			{
				"type": "object",
				"title": "PVS: Prototype Verification System",
				"properties": {
					"pvs.path": {
						"type": "string",
						"default": "",
						"description": "PVS installation folder"
					},
					"pvs.settings.completionProvider": {
						"type": "boolean",
						"default": true,
						"description": "Enable completion provider."
					},
					"pvs.settings.definitionProvider": {
						"type": "boolean",
						"default": true,
						"description": "Enable definition provider."
					},
					"pvs.settings.hoverProvider": {
						"type": "boolean",
						"default": true,
						"description": "Enable hover provider."
					},
					"pvs.settings.codelensProvider": {
						"type": "boolean",
						"default": true,
						"description": "Enable literate programming."
					}
				}
			}
		],
		"iconThemes": [
			{
				"id": "pvs",
				"label": "PVS",
				"path": "./icons/pvs-icons-theme.json"
			}
		],
		"todo": {
			"viewsContainers": {
				"activitybar": [
					{
						"id": "pvs-commands-palette",
						"title": "PVS Commands Palette",
						"icon": "icons/pvs.svg"
					}
				]
			}
		},
		"views": {
			"explorer": [
				{
					"id": "theory-explorer-view",
					"name": "PVS Theory Explorer",
					"when": "resourceLangId == pvs"
				},
				{
					"id": "proof-explorer-view",
					"name": "PVS Proof Explorer",
					"when": "resourceLangId == pvs && prover-session-active"
				},
				{
					"id": "proofmate-view",
					"name": "PVS Proof Mate",
					"when": "resourceLangId == pvs && prover-session-active"
				}
			]
		},
		"snippets": [
			{
				"language": "pvs",
				"path": "./snippets.json"
			}
		],
		"languageServerExample.trace.server": {
			"scope": "window",
			"anyOf": [
				{
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off"
				},
				{
					"type": "object",
					"properties": {
						"verbosity": {
							"type": "string",
							"enum": [
								"off",
								"messages",
								"verbose"
							],
							"default": "off"
						},
						"format": {
							"type": "string",
							"enum": [
								"text",
								"json"
							],
							"default": "text"
						}
					}
				}
			],
			"default": "off",
			"description": "Traces the communication between VS Code and the language server."
		},
		"commands": [
			{
				"command": "vscode-pvs.typecheck-file",
				"title": "Typecheck"
			},
			{
				"command": "vscode-pvs.parse-file",
				"title": "Parse File"
			},
			{
				"command": "vscode-pvs.parse-workspace",
				"title": "Parse Workspace"
			},
			{
				"command": "vscode-pvs.hp2pvs",
				"title": "Generate PVS file"
			},
			{
				"command": "vscode-pvs.prove-formula",
				"title": "Prove"
			},
			{
				"command": "vscode-pvs.pvsio-evaluator",
				"title": "Evaluate in PVSio"
			},
			{
				"command": "proof-explorer.jump-to",
				"title": "Jump here"
			},
			{
				"command": "proof-explorer.delete-proof-command",
				"title": "Delete"
			},
			{
				"command": "proof-explorer.rename-proof-command",
				"title": "Rename"
			},
			{
				"command": "proof-explorer.cut-proof-command",
				"title": "Cut"
			},
			{
				"command": "proof-explorer.copy-proof-command",
				"title": "Copy"
			},
			{
				"command": "proof-explorer.paste-proof-command",
				"title": "Paste"
			},
			{
				"command": "proof-explorer.back",
				"title": "Back one step",
				"icon": "icons/prev-icon.png"
			},
			{
				"command": "proof-explorer.forward",
				"title": "Step proof",
				"icon": "icons/next-icon.png"
			},
			{
				"command": "proof-explorer.run",
				"title": "️️Run proof",
				"icon": "icons/play-circle-1-icon.png"
			},
			{
				"command": "proof-explorer.fast-forward",
				"title": "️️Fast forward",
				"icon": "icons/fast-forward-icon.png"
			},
			{
				"command": "vscode-pvs.editor-show-sequent",
				"title": "️️Show sequent"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "vscode-pvs.typecheck-file",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				}
			],
			"view/item/context": [
				{
					"command": "vscode-pvs.prove-formula",
					"when": "view == theory-explorer-view && viewItem == tcc",
					"group": "inline"
				},
				{
					"command": "vscode-pvs.prove-formula",
					"when": "view == theory-explorer-view && viewItem == theorem",
					"group": "inline"
				},
				{
					"command": "vscode-pvs.typecheck-file",
					"when": "view == theory-explorer-view && viewItem == theory",
					"group": "inline"
				},
				{
					"command": "vscode-pvs.typecheck-file",
					"when": "view == theory-explorer-view && viewItem == theory",
					"group": "context-menu"
				},
				{
					"command": "proof-explorer.fast-forward",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/ctrl"
				},
				{
					"command": "proof-explorer.fast-forward",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/ctrl"
				},
				{
					"command": "proof-explorer.jump-to",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/ctrl"
				},
				{
					"command": "proof-explorer.jump-to",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/ctrl"
				},
				{
					"command": "proof-explorer.run",
					"when": "view == proof-explorer-view && viewItem == root",
					"group": "context-menu/ctrl"
				},
				{
					"command": "proof-explorer.delete-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/edit-1"
				},
				{
					"command": "proof-explorer.delete-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/edit-1"
				},
				{
					"command": "proof-explorer.rename-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/edit-1"
				},
				{
					"command": "proof-explorer.rename-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/edit-1"
				},
				{
					"command": "proof-explorer.cut-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/edit-0"
				},
				{
					"command": "proof-explorer.cut-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/edit-0"
				},
				{
					"command": "proof-explorer.copy-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/edit-0"
				},
				{
					"command": "proof-explorer.copy-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/edit-0"
				},
				{
					"command": "proof-explorer.paste-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-command",
					"group": "context-menu/edit-0"
				},
				{
					"command": "proof-explorer.paste-proof-command",
					"when": "view == proof-explorer-view && viewItem == proof-branch",
					"group": "context-menu/edit-0"
				}
			],
			"editor/title/context": [
				{
					"command": "vscode-pvs.typecheck-file",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				},
				{
					"command": "vscode-pvs.parse-file",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				},
				{
					"command": "vscode-pvs.parse-file",
					"when": "resourceScheme == file && resourceExtname == .hpvs"
				},
				{
					"command": "vscode-pvs.parse-workspace",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				},
				{
					"command": "vscode-pvs.hp2pvs",
					"when": "resourceScheme == file && resourceExtname == .hpvs"
				}
			],
			"editor/context": [
				{
					"command": "vscode-pvs.typecheck-file",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				},
				{
					"command": "vscode-pvs.parse-file",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				},
				{
					"command": "vscode-pvs.parse-workspace",
					"when": "resourceScheme == file && resourceExtname == .pvs"
				},
				{
					"command": "vscode-pvs.parse-file",
					"when": "resourceScheme == file && resourceExtname == .hpvs"
				},
				{
					"command": "vscode-pvs.hp2pvs",
					"when": "resourceScheme == file && resourceExtname == .hpvs"
				}
			],
			"view/title": [
				{
					"command": "proof-explorer.back",
					"when": "view == proof-explorer-view",
					"group": "navigation"
				},
				{
					"command": "proof-explorer.forward",
					"when": "view == proof-explorer-view",
					"group": "navigation"
				},
				{
					"command": "proof-explorer.run",
					"when": "view == proof-explorer-view",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"key": "alt+x",
				"command": "vscode-pvs.metax",
				"when": "resourceLangId == pvs"
			},
			{
				"key": "shift+alt+x",
				"command": "vscode-pvs.metax",
				"when": "resourceLangId == pvs"
			},
			{
				"key": "alt+x",
				"command": "vscode-pvs.metax",
				"when": "resourceLangId == pvs"
			},
			{
				"key": "shift+alt+x",
				"command": "vscode-pvs.metax",
				"when": "resourceLangId == pvs"
			},
			{
				"key": "ctrl+t",
				"command": "vscode-pvs.typecheck-file",
				"when": "resourceLangId == pvs"
			},
			{
				"key": "ctrl+alt+g",
				"command": "proof-explorer.grind",
				"when": "resourceLangId == pvs"
			},
			{
				"key": "ctrl+c",
				"command": "workbench.action.terminal.sendSequence",
				"args": {
					"text": ""
				},
				"note": "overriding default vscode command, which kills the terminal",
				"mnemonic": "TODO: redirect to ctrl+shift+c",
				"when": "terminalFocus && resourceLangId == pvs"
			},
			{
				"key": "ctrl+c",
				"command": "proof-explorer.copy-proof-command",
				"args": {
					"text": ""
				},
				"note": "copy proof command in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			},
			{
				"key": "ctrl+x",
				"command": "proof-explorer.cut-proof-command",
				"args": {
					"text": ""
				},
				"note": "cut proof command in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			},
			{
				"key": "ctrl+v",
				"command": "proof-explorer.paste-proof-command",
				"args": {
					"text": ""
				},
				"note": "paste proof command in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			},
			{
				"key": "F2",
				"command": "proof-explorer.rename-proof-command",
				"args": {
					"text": ""
				},
				"note": "rename proof command in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			},
			{
				"key": "F5",
				"command": "proof-explorer.run",
				"args": {
					"text": ""
				},
				"note": "run proof in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			},
			{
				"key": "F6",
				"command": "proof-explorer.forward",
				"args": {
					"text": ""
				},
				"note": "step proof in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			},
			{
				"key": "F4",
				"command": "proof-explorer.back",
				"args": {
					"text": ""
				},
				"note": "one step back in proof explorer",
				"when": "view == proof-explorer-view && viewItem == proof-command"
			}
		],
		"languages": [
			{
				"id": "pvs",
				"extensions": [
					".pvs",
					".tccs",
					".ppe",
					".prf",
					".pr",
					".hpvs"
				],
				"configuration": "./syntax/pvs-language-configuration.json",
				"aliases": [
					"PVS"
				]
			}
		],
		"grammars": [
			{
				"language": "pvs",
				"scopeName": "source.pvs",
				"path": "./syntax/pvs-language.json"
			}
		],
		"colors": [
			{
				"id": "pvs.keywords.color",
				"description": "Decoration color for reserved keywords",
				"defaults": {
					"dark": "#3fa9ff",
					"light": "#4627ce55",
					"highContrast": "#2450af55"
				}
			},
			{
				"id": "pvs.operators.color",
				"description": "Decoration color for operators",
				"defaults": {
					"dark": "#3fa9ff",
					"light": "#4627ce55",
					"highContrast": "#2450af55"
				}
			},
			{
				"id": "pvs.builtinTypes.color",
				"description": "Decoration color for basic types",
				"defaults": {
					"dark": "#31d18e",
					"light": "#27975fd3",
					"highContrast": "#27975fd3"
				}
			},
			{
				"id": "pvs.functions.color",
				"description": "Decoration color for library functions",
				"defaults": {
					"dark": "#31d18e",
					"light": "#27975fd3",
					"highContrast": "#27975fd3"
				}
			},
			{
				"id": "pvs.comments.color",
				"description": "Decoration color for basic types",
				"defaults": {
					"dark": "#29a385e7",
					"light": "#174b40a8",
					"highContrast": "#1d4b17a8"
				}
			},
			{
				"id": "pvs.numbers.color",
				"description": "Decoration color for numbers",
				"defaults": {
					"dark": "#e0f584",
					"light": "#7d8a43f8",
					"highContrast": "#7d8a43f8"
				}
			},
			{
				"id": "pvs.strings.color",
				"description": "Decoration color for numbers",
				"defaults": {
					"dark": "#f5c484",
					"light": "#8a7443f8",
					"highContrast": "#8a7443f8"
				}
			},
			{
				"id": "pvs.constants.color",
				"description": "Decoration color for constants",
				"defaults": {
					"dark": "#3fa9ff",
					"light": "#4627ce55",
					"highContrast": "#2450af55"
				}
			},
			{
				"id": "pvs.error.syntax",
				"description": "Decoration color for syntax errors",
				"defaults": {
					"dark": "#ff3fc5d2",
					"light": "#ce279c4b",
					"highContrast": "#af248155"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
		"test:server": "node_modules/jasmine-ts/lib/index.js --config=server/test/specs/jasmine-test-all.json"
	},
	"devDependencies": {
		"@types/node": "12.12.2",
		"@types/jasmine": "^3.5.0",
		"@types/vscode": "^1.31.0"
	},
	"testDependencies": {
		"jasmine": "^3.5.0",
		"jasmine-ts": "^0.3.0"
	},
	"_obsolete_dev_dependencies_": {
		"ts-node": "^8.5.4",
		"tslint": "^5.20.1"
	},
	"dependencies": {
		"typescript": "^3.7.3",
		"vsce": "^1.71.0",
		"xmlrpc": "^1.3.2"
	}
}
