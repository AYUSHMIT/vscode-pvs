{
	"name": "vscode-pvs",
	"description": "Integrates the PVS theorem prover into Visual Studio Code",
	"version": "1.0.15-beta",
	"author": "Paolo Masci",
	"displayName": "PVS",
	"icon": "icons/pvs-icon.png",
	"license": "NASA-1.3",
